# Скопируйте этот файл в .env и заполните своими данными
# cp .env.example .env

# Telegram боты (получить у @BotFather)
MAIN_BOT_TOKEN=your_main_bot_token_here
AI_BOT_TOKEN=your_ai_bot_token_here

